<!DOCTYPE html>
<html lang="en">

<head>
  <%- include("./layout/layout.css.ejs") %>
</head>

<body>

  <%- include("./layout/layout.header.ejs") %>
    <div class="row ">
      <div class="col-12 grid-margin">
        <form action="../../producer/updateproducer/<%= producers.id %>" method="POST" enctype="application/x-www-form-urlencoded">
          <div class="form-group">
            <input type="hidden" class="form-control" name="id" value="<%= producers.id %>" id="exampleInputEmail1">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Producer Name</label>
            <input type="text" class="form-control" name="producername" value="<%= producers.producerName %>"
              id="exampleInputPassword1">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Address</label>
            <input type="text" class="form-control" name="address" value="<%= producers.address %>"
              id="exampleInputPassword1">
          </div>
          <button type="submit" class="btn btn-primary">Update</button>&nbsp;&nbsp;&nbsp;&nbsp;
          <a href="/producer/listproducer" class="btn btn-primary">Back</a>
        </form>
      </div>
    </div>
    <%- include("./layout/layout.footer.ejs") %>
      <%- include("./layout/layout.js.ejs") %>

</body>

</html>